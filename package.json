{
  "name": "mix-cn",
  "type": "module",
  "version": "0.0.2",
  "main": "cjs/index.cjs",
  "typings": "types/index.d.ts",
  "author": "earthnut  <earthnut.dev@outlook.com>  (https://earthnut.dev/mix-cn)",
  "description": "A tool that mix the class attribute of an HTML element node",
  "scripts": {
    "b": "rollup --config rollup.config.js && tsc -p tsconfig.types.json",
    "build": "npx ixxx rm dist run b",
    "diff": "npx ixxx pkg diff",
    "eslint": "npx ixxx cls && eslint src",
    "beautify": "npm run prettier",
    "prettier": "npx ixxx cls && prettier . --write",
    "test": "npx ixxx cls && rollup --config rollup.config.test.js && node test/out/test/index.mjs"
  },
  "files": [
    "mjs",
    "cjs",
    "types"
  ],
  "exports": {
    ".": {
      "import": {
        "default": "./mjs/index.mjs",
        "types": "./types/index.d.ts"
      },
      "require": {
        "default": "./cjs/index.cjs",
        "types": "./types/index.d.ts"
      }
    }
  },
  "keywords": [
    "mix-cn"
  ],
  "license": "ISC",
  "homepage": "https://earthnut.dev/mix-cn",
  "bugs": {
    "url": "https://github.com/earthnutDev/mix-cn/issues",
    "email": "earthnut.dev@outlook.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/earthnutDev/mix-cn.git')"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {},
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.14.13",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "rollup": "^4.19.2",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.5.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.18.0"
  }
}